# ===================================
# Tawk Voice Agents SDK Configuration
# ===================================

# ===================================
# LLM Provider API Keys
# ===================================

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google AI Configuration
GOOGLE_GENERATIVE_AI_API_KEY=your_google_api_key_here
GEMINI_API_KEY=your_google_api_key_here

# Groq Configuration
GROQ_API_KEY=your_groq_api_key_here

# ===================================
# Speech-to-Text (STT) Providers
# ===================================

# Deepgram Configuration
DEEPGRAM_API_KEY=your_deepgram_api_key_here

# AssemblyAI Configuration
ASSEMBLYAI_API_KEY=your_assemblyai_api_key_here

# ===================================
# Text-to-Speech (TTS) Providers
# ===================================

# ElevenLabs Configuration
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
ELEVENLABS_VOICE_ID=your_voice_id_here

# Cartesia Configuration
CARTESIA_API_KEY=your_cartesia_api_key_here

# Azure Speech Configuration
AZURE_SPEECH_KEY=your_azure_speech_key_here
AZURE_SPEECH_REGION=your_azure_region_here

# ===================================
# Tracing & Observability
# ===================================

# Langfuse Configuration (Optional)
LANGFUSE_SECRET_KEY=your_langfuse_secret_key_here
LANGFUSE_PUBLIC_KEY=your_langfuse_public_key_here
LANGFUSE_BASE_URL=https://cloud.langfuse.com

# ===================================
# Session Storage
# ===================================

# Redis Configuration (Optional - for RedisSession)
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password_here

# MongoDB Configuration (Optional - for DatabaseSession)
MONGODB_URI=mongodb://localhost:27017/voice-agents
MONGODB_DATABASE=voice-agents

# ===================================
# Server Configuration
# ===================================

# Server Port
PORT=8080

# WebSocket Configuration
WS_HOST=localhost
WS_PORT=8080

# WebRTC Configuration
WEBRTC_HOST=localhost
WEBRTC_PORT=8081

# ===================================
# Application Settings
# ===================================

# Environment
NODE_ENV=development

# Logging Level (debug, info, warn, error)
LOG_LEVEL=info

# API Authentication (Optional)
API_KEY=your_api_key_here

# ===================================
# Development & Testing
# ===================================

# Test Configuration
TEST_TIMEOUT=30000
SKIP_INTEGRATION_TESTS=false
SKIP_E2E_TESTS=false
