{
  "name": "@tawk/voice-agents-sdk",
  "version": "1.0.0",
  "description": "Production-ready Voice AI SDK with WebSocket and WebRTC server-to-server support, powered by Tawk Agents SDK for agent orchestration and multi-provider STT/TTS integration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./transport": {
      "types": "./dist/transport/index.d.ts",
      "import": "./dist/transport/index.js",
      "require": "./dist/transport/index.js"
    },
    "./providers": {
      "types": "./dist/providers/index.d.ts",
      "import": "./dist/providers/index.js",
      "require": "./dist/providers/index.js"
    },
    "./voice-agent": {
      "types": "./dist/voice-agent/index.d.ts",
      "import": "./dist/voice-agent/index.js",
      "require": "./dist/voice-agent/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js",
      "require": "./dist/server/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js",
      "require": "./dist/client/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node",
    "start:server": "ts-node examples/server.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:performance": "vitest run tests/performance",
    "test:e2e": "vitest run tests/e2e",
    "test:providers": "ts-node examples/test-providers.ts",
    "test:simple": "ts-node examples/simple-test.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.ts\"",
    "clean": "rm -rf dist",
    "prepublish": "npm run clean && npm run build"
  },
  "keywords": [
    "voice-ai",
    "voice-agent",
    "websocket",
    "webrtc",
    "mediasoup",
    "realtime-api",
    "speech-to-text",
    "text-to-speech",
    "llm",
    "conversational-ai",
    "openai-realtime",
    "deepgram",
    "elevenlabs",
    "ai-sdk"
  ],
  "author": "Tawk",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tawk/tawk-voice-agents-sdk"
  },
  "bugs": {
    "url": "https://github.com/tawk/tawk-voice-agents-sdk/issues"
  },
  "homepage": "https://github.com/tawk/tawk-voice-agents-sdk#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@ai-sdk/anthropic": "^1.0.0",
    "@ai-sdk/google": "^1.0.0",
    "@ai-sdk/groq": "^1.0.0",
    "@ai-sdk/openai": "^1.0.0",
    "@deepgram/sdk": "^3.4.0",
    "ai": "^4.1.0",
    "assemblyai": "^4.6.0",
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "eventemitter3": "^5.0.1",
    "isomorphic-ws": "^5.0.0",
    "langfuse": "^3.30.3",
    "openai": "^4.68.0",
    "ws": "^8.18.0",
    "zod": "^3.25.40"
  },
  "optionalDependencies": {
    "@modelcontextprotocol/sdk": "^1.17.2",
    "ioredis": "^5.4.2",
    "mongodb": "^6.12.0",
    "onnxruntime-node": "^1.19.0",
    "wrtc": "^0.4.7"
  },
  "devDependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.17",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/ui": "^4.0.8",
    "c8": "^10.1.3",
    "eslint": "^8.56.0",
    "fluent-ffmpeg": "^2.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^4.0.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
